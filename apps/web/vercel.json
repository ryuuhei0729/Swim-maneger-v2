{
  "version": 2,
  "buildCommand": "cd ../.. && npm run graphql:codegen && cd apps/web && npm run build",
  "outputDirectory": ".next",
  "installCommand": "cd ../.. && npm ci",
  "framework": "nextjs",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./",
  "env": {
    "NEXT_PUBLIC_ENVIRONMENT": "production"
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_ENVIRONMENT": "production"
    }
  },
  "git": {
    "deploymentEnabled": {
      "main": true,
      "develop": true
    }
  }
}
