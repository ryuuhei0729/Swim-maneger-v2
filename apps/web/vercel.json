{
  "version": 2,
  "buildCommand": "cd ../.. && npm run build:web",
  "outputDirectory": "apps/web/.next",
  "installCommand": "cd ../.. && npm install",
  "framework": "nextjs",
  "env": {
    "NEXT_PUBLIC_ENVIRONMENT": "production"
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_ENVIRONMENT": "production"
    }
  },
  "preview": {
    "env": {
      "NEXT_PUBLIC_ENVIRONMENT": "preview"
    }
  },
  "git": {
    "deploymentEnabled": {
      "main": true,
      "develop": true
    }
  }
}
